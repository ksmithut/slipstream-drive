{
  "name": "slipstream-drive",
  "version": "1.0.0",
  "description": "An RPC framework that leverages protocol buffers to make server to server communication fast.",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run test:lint",
    "test": "npm run test:unit",
    "posttest": "npm run test:coverage",
    "test:lint": "eslint . --ignore-path=.gitignore",
    "test:unit": "NODE_ENV=test istanbul cover _mocha",
    "test:coverage": "istanbul check-coverage",
    "test:coverage:report": "open coverage/lcov-report/index.html",
    "test:coverage:codeclimate": "cat ./coverage/lcov.info | codeclimate-test-reporter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksmithut/slipstream-drive.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithut/slipstream-drive/issues"
  },
  "homepage": "https://github.com/ksmithut/slipstream-drive#readme",
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "protocol-buffers": "^3.1.6",
    "request": "^2.73.0",
    "request-promise": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-spies": "^0.7.1",
    "codeclimate-test-reporter": "^0.3.3",
    "eslint": "^3.0.1",
    "eslint-config-ksmithut": "^2.1.3",
    "eslint-plugin-mocha": "^4.0.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "nock": "^8.0.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.2.0"
  }
}
